{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/claim-relayer",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/claim-relayer",
        "commands": [
          "tsc -p services/claim-relayer/tsconfig.lib.json",
          "tsc-alias -p services/claim-relayer/tsconfig.lib.json"
        ],
        "parallel": false
      }
    },
    "dev:publisher": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node --project services/claim-relayer/tsconfig.json -r tsconfig-paths/register services/claim-relayer/commands/claim-publisher.ts"
        ]
      }
    },
    "dev:subscriber": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node --project services/claim-relayer/tsconfig.json -r tsconfig-paths/register services/claim-relayer/commands/claim-subscriber.ts"
        ]
      }
    },
    "dev:eth-poller": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node --project services/claim-relayer/tsconfig.json -r tsconfig-paths/register services/claim-relayer/commands/eth-poller.ts"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/claim-relayer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/services/claim-relayer"],
      "options": {
        "jestConfig": "services/claim-relayer/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
